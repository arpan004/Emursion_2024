Problem Link: https://leetcode.com/problems/reverse-integer/submissions/1141497403/


class Solution {
public:
    long int reverse(int x) {
        long int sum=0;
        for(int i=0;x;i++)
        {
            sum=sum*10+x%10;
            x/=10;
        }
        if(sum >=-2147483648 && sum <= 2147483647)
            return sum;
        else
            return 0;
    }
};
